{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-react"],
  "jsRules": {},
  "rules": {
    "member-access": true,
    "ordered-imports": true,
    "quotemark": true,
    "no-console": true,
    "semicolon": true,
    "jsx-no-lambda": true
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  }
}
